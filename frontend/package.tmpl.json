{
  "name": "pocgosveltekit",
  "version": "0.0.0",
  "type": "module",
  "author": "",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "prepare": "svelte-kit sync",
